# The base URL of your Paperless-ngx instance (e.g., http://192.168.1.10:8000).
PAPERLESS_API_URL=http://your-paperless-instance.com

# Your Paperless-ngx API token. You can create one in the Paperless admin settings.
PAPERLESS_API_TOKEN=your_secret_api_token

# The ID of the custom field in Paperless where the SHA256 hash will be stored.
PAPERLESS_CUSTOM_FIELD_ID=1

# The logging level for the console output. Can be DEBUG, INFO, WARNING, ERROR.
LOG_LEVEL=INFO